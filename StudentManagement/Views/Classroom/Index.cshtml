@model IEnumerable<StudentMangament.Core.Models.Student>

@{
    ViewData["Title"] = $"Students in 6A {TempData["catName"]}";
}

<h2 class="post-title">Students in 6A @TempData["catName"]</h2>
<hr class="my-4" />

<table style="font-family: arial, sans-serif;border-collapse: collapse;width: 100%;">
        <tr>
        <th style="border: 1px solid #dddddd;text-align: left;padding: 8px;">Name</th>
        <th style="border: 1px solid #dddddd;text-align: left;padding: 8px;">Gender</th>
        <th style="border: 1px solid #dddddd;text-align: left;padding: 8px;">Date of Birth</th>
        <th style="border: 1px solid #dddddd;text-align: left;padding: 8px;">Phone Number</th>
        </tr>
        @foreach (var item in Model)
        {
        <tr>
            <td style="border: 1px solid #dddddd;text-align: left;padding: 8px;">@item.Name</td>
            <td style="border: 1px solid #dddddd;text-align: left;padding: 8px;">@item.Gender</td>
            <td style="border: 1px solid #dddddd;text-align: left;padding: 8px;">@item.DateofBirth</td>
            <td style="border: 1px solid #dddddd;text-align: left;padding: 8px;">@item.PhoneNumber</td>
        </tr>
        }
    </table>
